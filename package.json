{
  "name": "nextjs-zero-api",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --output src/lib --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore --output src/lib && next build",
    "start": "next start",
    "serve": "run-s build start",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "unit": "jest",
    "e2e": "playwright test",
    "test": "run-p -l -c --aggregate-output lint prettier unit",
    "fix": "run-s -l -c fix:lint fix:prettier",
    "fix:lint": "next lint --fix",
    "fix:prettier": "prettier --write .",
    "env": "vc env pull .env.local",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@tabler/icons-react": "^2.24.0",
    "intersection-observer": "^0.12.2",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-responsive": "^9.0.2",
    "rehype-parse": "^8.0.4",
    "rehype-react": "^7.2.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@playwright/test": "1.34.3",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "0.5.9",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "29.5.1",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-test-renderer": "18.0.0",
    "@types/testing-library__jest-dom": "^5.14.7",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-access": "^1.1.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "13.2.2",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "pathpida": "0.20.1",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-test-renderer": "^18.2.0",
    "tailwind-variants": "^0.1.10",
    "tailwindcss": "^3.3.2",
    "tailwindcss-radix-colors": "^0.3.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write ."
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  }
}
